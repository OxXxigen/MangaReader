{
	"name": "Manga reader",
	"version": "0.2",
	"manifest_version": 2,
	"default_locale": "en",
	"app": {
		"launch": {
			"local_path": "main.html"
		}
	},
	"permissions": [
		"storage",
		"unlimitedStorage",
		"<all_urls>"
	],
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}